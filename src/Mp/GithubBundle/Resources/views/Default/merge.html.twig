{% extends 'MpGithubBundle:Default:index.html.twig' %}

{% block content %}
    {{ form_start(form) }}
    {{ form_errors(form) }}

    <div class="form-group">
        {{ form_row(form.title) }}
    </div>

    <div class="form-group">
        {{ form_row(form.body) }}
    </div>

    <div class="form-group">
        {{ form_row(form.base) }}
    </div>

    <div class="form-group">
        {{ form_row(form.head) }}
    </div>

    {{ form_row(form.merge) }}
    {{ form_end(form) }}
{% endblock %}