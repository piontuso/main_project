{% include 'default/header.html.twig' %}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            {% for type, messages in app.session.flashBag.all %}
                {% for message in messages %}
                    <div class="alert alert-{{ type }}" role="alert">
                        {{ message|trans({}, 'FOSUserBundle') }}
                    </div>
                {% endfor %}
            {% endfor %}
        </div>
    </div>
    {% block body %}

    {% endblock %}
</div>
{% include 'default/footer.html.twig' %}

